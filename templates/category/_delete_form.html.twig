<form 
    class="d-inline-block"
    method="post" 
    action="{{ path('app_category_delete', {'id': category.id}) }}" 
    onsubmit="return confirm('Etes-vous sûr de vouloir cette catégorie?');"
>
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.id) }}">
    <button class="text-decoration-none" >
       <img src="{{ asset('images/supp.png')}}" alt="supprimer" class="sizeSvg30">
    </button>
</form>
 